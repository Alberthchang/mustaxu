extends ../layout

block content
	.container
		.row
			.col-md-12
				.alert.alert-success
					用户名 m 密码 m 欢迎测试
		.row
			.col-md-8
					each u in article
						div
							a(href="/p/#{u.id}")
								h4 #{u.title}
							p #{u.content}
						hr
			.col-md-4
					span 
		.row
			nav
				ul.pagination
					- for (var i = 1; i < pageNum+1; ++i) {
						li: a(href="/page/#{i}") #{i} 
					- }